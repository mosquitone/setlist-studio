{
  "name": "setlist-studio",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "pnpm generate:schema && next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "generate": "dotenv -e .env.local -- prisma generate",
    "generate:schema": "dotenv -e .env.local -- tsx scripts/generate-schema.ts",
    "db:push": "dotenv -e .env.local -- prisma db push",
    "db:migrate": "dotenv -e .env.local -- prisma migrate dev",
    "db:migrate:create": "dotenv -e .env.local -- prisma migrate dev --create-only",
    "db:migrate:deploy": "dotenv -e .env.local -- prisma migrate deploy",
    "db:migrate:status": "dotenv -e .env.local -- prisma migrate status",
    "db:migrate:reset": "dotenv -e .env.local -- prisma migrate reset",
    "db:studio": "dotenv -e .env.local -- prisma studio",
    "db:setup": "bash scripts/setup-db.sh",
    "db:setup:shadow": "bash scripts/setup-shadow-db.sh",
    "db:migrate:baseline": "bash scripts/baseline-migrations.sh",
    "db:prod:migrate:deploy": "dotenv -e .env.vercel -- prisma migrate deploy",
    "db:prod:migrate:status": "dotenv -e .env.vercel -- prisma migrate status",
    "db:prod:push": "dotenv -e .env.vercel -- prisma db push",
    "db:prod:studio": "dotenv -e .env.vercel -- prisma studio",
    "postinstall": "prisma generate"
  },
  "dependencies": {
    "@apollo/client": "^3.13.8",
    "@apollo/server": "^4.12.2",
    "@as-integrations/next": "^3.2.0",
    "@emotion/react": "^11",
    "@emotion/styled": "^11",
    "@hello-pangea/dnd": "^18.0.1",
    "@mui/icons-material": "^5.17.1",
    "@mui/lab": "7.0.0-beta.14",
    "@mui/material": "^5.17.1",
    "@mui/system": "^5.17.1",
    "@mui/x-date-pickers": "^8.9.0",
    "@mui/x-date-pickers-pro": "^8.9.0",
    "@prisma/client": "^6.12.0",
    "@vercel/speed-insights": "^1.2.0",
    "bcryptjs": "^3.0.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "date-fns": "^4.1.0",
    "dayjs": "^1.11.13",
    "dompurify": "^3.2.6",
    "formik": "^2.4.5",
    "graphql": "^15.8.0",
    "graphql-depth-limit": "^1.1.0",
    "html-to-image": "^1.11.13",
    "jsonwebtoken": "^9.0.2",
    "next": "15.4.4",
    "next-auth": "^4.24.11",
    "qrcode": "^1.5.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "reflect-metadata": "^0.2.2",
    "resend": "^4.7.0",
    "schema-dts": "^1.1.5",
    "type-graphql": "1.1.1",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@next/eslint-plugin-next": "^15.4.4",
    "@types/graphql-depth-limit": "^1.1.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20",
    "@types/qrcode": "^1.5.5",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "@vitest/ui": "^3.2.4",
    "dotenv-cli": "^8.0.0",
    "eslint": "^9.32.0",
    "eslint-config-next": "15.4.4",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.5.3",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "prettier": "^3.6.2",
    "prisma": "^6.12.0",
    "terser-webpack-plugin": "^5.3.14",
    "tsx": "^4.20.3",
    "typescript": "^5",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": "20.19.3"
  },
  "packageManager": "pnpm@10.12.1",
  "pnpm": {
    "overrides": {
      "graphql": "^15.8.0",
      "form-data": ">=4.0.4"
    }
  }
}
